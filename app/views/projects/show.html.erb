<h1><%=@project.name %></h1>

<p>Company: <%=@project.company.name %></p>
<p>Default Rate: <%=@project.default_rate%></p>
<p>Slug: <%= @project.slug %></p>

<table>
    <tbody>
        <tr>
            <th>View</th>
            <th>User</th>
            <th>Date/Time</th>
            <th>Hours</th>
        </tr>
        <% @project.works.each do |work|%>
            <%= render partial: 'works/work', collection: @project.works %>
        <% end %>
    </tbody>
</table>

<% content_for :aside do %>
 <% render 'sidebar' %>
<% end %>
